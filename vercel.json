{
  "version": 2,
  "builds": [
    { "src": "api/status.py", "use": "@vercel/python" },
    { "src": "api/export.py", "use": "@vercel/python" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/status", "dest": "api/status.py" },
    { "src": "/api/export", "dest": "api/export.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
