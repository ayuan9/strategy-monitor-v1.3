{
  "version": 2,
  "builds": [
    {
      "src": "api/status.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    },
    {
      "src": "api/export.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    { "src": "/api/status", "dest": "api/status.py" },
    { "src": "/api/export", "dest": "api/export.py" }
  ]
}
